{
  "name": "tourze/captcha-challenge-bundle",
  "description": "图片验证码模块",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-gd": "*",
    "gregwar/captcha": "^1.2",
    "nzo/url-encryptor-bundle": "^6.4",
    "symfony/cache": "^6.4",
    "symfony/cache-contracts": "^3",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/http-foundation": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/routing": "^6.4",
    "symfony/uid": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/json-rpc-core": "0.0.*",
    "tourze/json-rpc-lock-bundle": "0.1.*",
    "tourze/json-rpc-log-bundle": "0.1.*",
    "tourze/symfony-routing-auto-loader-bundle": "0.0.*"
